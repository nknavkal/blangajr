{
  "manifest_version": 2,

  "name": "rhubarber",
  "description": "rhubarbs the text",
  "version": "1.0",

  "background": {
    "scripts": [
      "another.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "image.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
